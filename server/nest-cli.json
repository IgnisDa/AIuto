{
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "plugins": [
      {
        "name": "@nestjs/swagger",
        "options": {
          "introspectComments": true,
          "dtoFileNameSuffix": [
            "input.ts",
            "dto.ts",
            "entity.ts"
          ]
        }
      }
    ]
  }
}
